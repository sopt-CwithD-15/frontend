  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "0.24.0",
    "prop-types": "15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-responsive": "9.0.0-beta.4",
    "react-router-dom": "6.0.2",
    "react-scripts": "4.0.3",
